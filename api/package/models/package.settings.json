{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "packages",
  "info": {
    "name": "package"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "label": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "price_unit": {
      "type": "string"
    },
    "services": {
      "type": "component",
      "repeatable": true,
      "component": "package.service"
    }
  }
}
