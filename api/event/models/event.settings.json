{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "events",
  "info": {
    "name": "event"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "date": {
      "type": "date"
    },
    "price": {
      "type": "decimal"
    },
    "location": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    }
  }
}
